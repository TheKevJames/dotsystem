Host github.com
    User git
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes

Host github.com-dialpad
    HostName github.com
    User git
    IdentityFile ~/.ssh/dialpad
    IdentitiesOnly yes

Host pi
    HostName 10.0.0.21
    User pi

Host titan
    HostName 192.168.0.110
    IdentityFile ~/.ssh/id_ed25519

